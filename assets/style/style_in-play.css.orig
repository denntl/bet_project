@import url(fonts.css);


/*~~~~~~~~~~~~~~~~~~ page_in-play~~~~~~~~~~~~~~~~~~~~~~*/
.home_page_header.in-play{
    justify-content: space-between;
}
.home_page_header.in-play:before{
    display: none;
}
.home_page_header .nav-tabs{
    border-bottom: none;
    padding-left: 15px;
}
.home_page_header .scoreboard_btn{
    font-size: 11px;
    color: #bbb;
    vertical-align: top;
    line-height: 29px;
    pointer-events: auto;
    padding-right: 25px;
    -webkit-font-smoothing: antialiased;
    text-decoration: none;
    transition: opacity .2s;
    cursor: pointer;
    opacity:0;
    z-index: -1;
}
.home_page_header .scoreboard_btn.appear{
    opacity:1;
    z-index: 1;
}
.tabs_match{
    border-bottom: 1px solid transparent;
    justify-content: center;
}
.page_right_content.in-play.stretch .tabs_match .nav-link,
.home_page_header .nav-tabs .nav-link{
    font-size: 12px;
    color: #bbb;
    display: inline-block;
    vertical-align: top;
    line-height: 29px;
    pointer-events: auto;
    padding:0;
    padding-right: 25px;
}
.page_right_content.in-play.stretch .tabs_match .nav-link{
    font-size: 13px;
    background-image: none;
    text-indent: inherit;
    width: 100%;
}
.page_right_content.in-play.stretch .tabs_match .nav-link:hover,
.page_right_content.in-play .tabs_match .nav-link:hover,
.page_right_content.in-play .tabs_match .nav-link.active,
.home_page_header .nav-tabs .nav-link:hover,
.home_page_header .nav-tabs .nav-link.active{
    color: #ffdf1b;
    background-color: transparent;
    border: 1px solid transparent;
}

.main_content.in-play .home_page{
    padding-bottom:0;
}
.main_content.in-play .column_one{
    min-width: 100%;
    max-width: 100%;
    width: 567px;
    border-right: 1px solid #373737;

}
.overview_tab_tables{
    padding-bottom: 60px;
    min-height: calc(100vh - 223px);
    background-color: #545454;
}

.play_classNameification.in-play .play_classification_header{
    justify-content: space-between;
}
.play_classification.in-play .play_classification_header .sport_events,
.play_classification.in-play .play_classification_header .markets_block{
    display: flex;
}
.play_classification.in-play .play_classification_header .sport_events .events_btn,
.play_classification.in-play .play_classification_header .markets_block .markets_btn{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    cursor: pointer;
    color: #ddd;
    font-size: 12px;
    opacity: .5;
    padding: 0 10px;
    line-height: 29px;
    vertical-align: middle;
}
.play_classification.in-play .play_classification_header .sport_events .events_btn:hover,
.play_classification.in-play .play_classification_header .sport_events .events_btn.selected,
.play_classification.in-play .play_classification_header .markets_block .markets_btn.selected{
    opacity: 1;
}
.play_classification.in-play .play_classification_header .markets_block .markets_btn:hover{
    color: #e4e4e4;
}
.participant_group_wrapper{
    display: table-cell;
    white-space: nowrap;
    background-color: #666;
    vertical-align: middle;
}
.play_classification.in-play .participant_group{
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-align-items: stretch;
    -ms-flex-align: stretch;
    align-items: stretch;
    height: 100%;
    max-height: 100%;
    background-color: #666;
}
.play_classification.in-play .participant_group .column{

    align-items: flex-start;
}

.extra_time{
    padding: 0 3px;
    color: #444;
    background-color: #a7a7a7;
    border-radius: 1px;
    margin-left: 10px;
    line-height: 14px;
    margin-top: 4px;
    font-size: 11px;
    display: inline-block;
    text-align: center;
}
.participant_goal{
    margin: 0 5px;
    padding: 1px 3px!important;
    background: #ffdf1b;
    font-size: 11px;
    color: #333;
    border-radius: 1px;
    line-height: 18px;
    display: inline-block!important;
}
.participant_stack{
    display: flex;
    width: 100%;
    align-items: center;
    justify-content: space-between;
    padding-right: 10px;
    padding-left: 10px;
}
.participant_wrapper{
    width:100%;
}
.play_icon_wrapper{
    padding-right: 10px;
    -webkit-flex: 0 0 auto;
    -ms-flex: 0 0 auto;
    flex: 0 0 auto;
    display: -webkit-inline-flex;
    display: -ms-inline-flexbox;
    display: inline-flex;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    flex-direction: column;
    height: 100%;
    min-height: 38px;
}
.play_icon{
    width: 23px;
    height: 13px;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-image: url(/img/ML1BW.svg);
}
.play_icon:hover,
.play_icon.selected{
    background-image: url(/img/ML1.svg);
}
.participant_group .column.data{
    width: 33.34%;
    padding-left: 0;
    flex-direction: column;
}
.column.data .participant_odds{
    background-color: #585858;
    font-size: 12px;
    width: 100%;
    cursor: pointer;
    padding: 0 10px;
    white-space: nowrap;
    border-bottom: 1px solid #757575;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
}
.column.data .participant_odds:hover{
    background-color: #707070;
}
.competition_btn{
    background-color: #a7a7a7;
    border-bottom: 1px solid transparent;
    font-size: 12px;
    color: #444;
    height: 25px;
    cursor: pointer;
    display: table;
    width: 100%;
    position: relative;
}
.competition_btn:after{
    content: " ";
    width: 50px;
    display: table-cell;
}
.competition_btn:hover{
    background-color: #bbb;
}
.competition_btn .league_name{
    display: table-cell;
    padding: 0 10px;
    line-height: inherit;
    vertical-align: middle;
}
.competition_btn .league_header_buttons{
    display: table-cell;
    vertical-align: middle;
    width: 55%;
    white-space: nowrap;
}
.competition_btn .league_header_buttons .event_label{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    display: inline-block;
    vertical-align: middle;
    width: 33%;
    text-align: center;
    padding: 0 5px;


}
.competition_btn .league_header_buttons{
    opacity: 0;
    transition-timing-function: cubic-bezier(.77,0,.175,1);
    transition-property: opacity;
    transition-duration: .35s;
}
.competition_btn .league_header_buttons.visible{
    opacity: 1;
}
.event_odds{
    width: 55%;
    display: table-cell;
    background-color: #585858;
    white-space: nowrap;
}
.play_league_group_wrapp.opened{
    display: table-row;
}
.play_league_group_wrapp.opened{
    display: table;
    width: 100%;
    table-layout: fixed;
    white-space: nowrap;
    border-bottom: 1px solid #757575;
}
.event_odds .column{
    display: flex;
    width: 100%;
}
.favourite_icon_wrapper{
    width: 50px;
    height: 14px;
    position: absolute;
    right: 0;
    top: 5px;

    cursor: pointer;
}
.favourite_icon{
   height: 20px;
   position: relative;
}
.favourite_icon:after{
    font-family: bet365UI;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\e80D";
    color: #888;
    position: absolute;
    top: 0;
    left: 18px;
    font-size: 14px;
    width: 15px;
}
.event_count_btn{
    display: table-cell;
    width: 49px;
    vertical-align: middle;
    text-align: center;
    background-color: #666;
    position: relative;
}
.event_count_btn .event_count{
    font-size: 12px;
    text-align: center;
    color: #bbb;
    line-height: 17px;
    cursor: pointer;
    display: block;
    padding-top: 4px;
}
.event_count_btn .event_count:after{
    font-family: bet365UI;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\e813";
    font-size: 9px;
    padding: 0 0 0 5px;
    top: -1px;
    position: relative;
}
.event_count_btn .event_count:hover{
    color: #e4e4e4;
}
/*~~~~~~~~~~~~~~~right column~~~~~~~~~~~~~~~~*/
.page_right_content.in-play .icon_buttons{
    padding: 0 31px 0 32px;
    text-align: center;
    font-size: 0;
    height: 29px;
    position: relative;
}
.page_right_content.in-play .icon_buttons .icon_sound{
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 32px;
    height: 29px;
    line-height: 29px;
    text-align: center;
    font-size: 10px;
    background-color: #474747;
    color: #999;
    border-left: 1px solid #545454;
    border-right: 1px solid #545454;
    cursor: pointer;
}
.video_control_container .lock_btn:hover,
.video_control_container .lock_btn.active,
.page_right_content.in-play .icon_buttons .icon_view:hover,
.page_right_content.in-play .icon_buttons .icon_sound:hover{
    color: #e4e4e4;;
}
.page_right_content.in-play .icon_buttons .icon_sound:before{
    font-family: bet365UI;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    content: "\e811";
}
.page_right_content.in-play .icon_buttons .icon_resize{
    display: block;
    position: absolute;
    top: 1px;
    right: 31px;
    width: 31px;
    height: 28px;
    line-height: 28px;
    text-align: center;
    font-size: 15px;
    background-color: #474747;
    border-left: 1px solid #545454;
    color: #999;
    cursor: pointer;
}
.page_right_content.in-play .icon_buttons .icon_view{
    display: block;
    position: absolute;
    top: 1px;
    right: 0;
    width: 31px;
    height: 28px;
    line-height: 28px;
    text-align: center;
    font-size: 5px;
    background-color: #474747;
    color: #999;
    border-left: 1px solid #545454;
    cursor: pointer;
}
.page_right_content.in-play .icon_buttons .icon_view:before{
    font-family: bet365UI;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\e801";
}
.page_right_content.in-play .nav-link{
    font-size: 13px;
    text-indent: -9999px;
    width: 23px;
    background-position: 0 50%;
    background-repeat: no-repeat;
    background-image: url(/img/live_stream_icon.svg);
}
.page_right_content.in-play .nav-link.match_live{
      background-image: url(/img/ML1BW.svg);
  }
.page_right_content.in-play .nav-link.active{
    background-image: url(/img/live_stream_icon_active.svg);
}
.page_right_content.in-play .nav-link.match_live:hover,
.page_right_content.in-play .nav-link.match_live.active{
    background-image: url(/img/ML1.svg);
}
.page_right_content.in-play .nav-link.live_stream:hover{
    background-image: url(/img/live_stream_icon_hover.svg);
}
.page_right_content.in-play.stretch .nav-link.live_stream:hover,
.page_right_content.in-play.stretch .nav-link.match_live:hover,
.page_right_content.in-play.stretch .nav-link.active{
    background-image: none;
}
/*~~~~~~~~~~~~~.live_stream tab~~~~~~~~~~~~~~~~*/
.live_stream_video{
    border-top: 1px solid #303030;
    position: relative;
    background-color: #000;
}
.video_control_container{
    display: table;
    table-layout: fixed;
    width: 100%;
    height: 31px;
    background-color: #545454;
    border-bottom: 1px solid #303030;
}
.tab-content{
    border-bottom-color: transparent;
    border-top-color: transparent;
    outline: none;
}
.video_control_container .select_video{
    display: table-cell;
    vertical-align: middle;
    position: relative;
    font-size: 12px;
    white-space: nowrap;
    width: 100%;
    height: 100%;
    -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    background-color: transparent;
    cursor: pointer;
}
.btn-group.select_video:after{
    font-family: bet365UI;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    display: block;
    position: absolute;
    right: 0;
    top: 15px;
    width: 10px;
    font-size: 5px;
    content: "\e801";
    color: #a7a7a7;
}
.btn-group.select_video{
    cursor: pointer;
}
.btn-group.select_video:hover:after{
    color: #ddd;
}
.video_control_container .select_video .option{
    color: #333;
    font-family: Verdana,sans-serif;
    text-align: left;
    text-overflow: ellipsis;
    overflow: hidden;
    display: block;
}
.video_control_container button.select_video .option{
    color: #ddd;
}
.dropdown-toggle::after{
    display: none;
}
.video_control_container .select_video:focus{
    -webkit-box-shadow: none;
    box-shadow: none;
}
.video_control_container .lock_btn{
    display: table-cell;
    width: 30px;
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    text-align: center;
    color: #a7a7a7;
    background-color: #545454;
    cursor: pointer;
}
.video_control_container .lock_btn:before{
    font-family: bet365UI-icomoon!important;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
     -webkit-font-smoothing: antialiased;
    content: "\e921";
}
.btn-group.select_video .dropdown-menu{
    background-color: #bbb;
    border-radius: 0 0 2px 2px;
    overflow: hidden;
    height: 180px;
    overflow-y: scroll;
    width: 100%;
}
.btn-group.select_video .dropdown-menu li{
    background-color: #bbb;
    line-height: 29px;
    padding: 0 10px 0 38px;
    cursor: pointer;
    transition: background-color .1s;
    transition-timing-function: ease-in-out;
    position: relative;
    border-top: 1px solid #a7a7a7;
}
.btn-group.select_video .dropdown-menu li:first-child{
    border-top: none;
}
.btn-group.select_video .dropdown-menu li:before{
    content: '';
    position: absolute;
    top: 0;
    left: 5px;
    width: 30px;
    height: 30px;
    background-size: auto 16px;
    background-position: 50% 50%;
    background-repeat: no-repeat;
}
.btn-group.select_video .dropdown-menu li.tennis_video:before{
    background-image: url(/img/tennis_icon.svg);
}
.btn-group.select_video .dropdown-menu li.football_video:before{
    background-image: url(/img/football_icon.svg);
}
.btn-group.select_video .dropdown-menu li a{
    font-size: 12px;
    color: #333;
}
.btn-group.select_video .dropdown-menu li:hover{
    background-color: #c7c7c7;
}
/*~~~~~~~~~~~~~~ live_stream player~~~~~~~~~~~~~*/

.page_right_content .media_player{
    width: 100%;
    height: 100%;
    position: relative;
    margin: 0 auto;
    background-color: #303030;
    padding-top: 168px;
}
.page_right_content.stretch .media_player{
    padding-top: 333px;
}
.media_player_container{
    text-align: center;
    width: 100%;
    height: 100%;
}
.media_player_wrapper{
    position: absolute;
    top: 0;
    height: 100%;
    width: 100%;
}
.media_player_message{
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
    padding: 0 16%;
    width: 100%;
}
.media_player_message_header{
    font-size: 15px;
    line-height: 22px;
    color: #e4e4e4;
}
.media_player_message_text{
    font-size: 13px;
    line-height: 19px;
    color: #c7c7c7;
    cursor: default;
}
/*~~~~~~~~~~~~~~ end live_stream player~~~~~~~~~~~~~*/


/*~~~~~~~~~~~~~~ match_live view~~~~~~~~~~~~~~*/
.match_live_view{
    background-color: #393939;
    border-top: 1px solid #303030;
}
.match_live_view_container{
    margin: 0 auto;
    max-width: 450px;
}
.match_live_view_module{
    transition-property: max-width,padding-left,padding-right;
    transition-duration: .65s;
    transition-timing-function: cubic-bezier(.77, 0, .175, 1);
    background-color: #393939;
    padding-left: 4px;
    padding-right: 4px;
    max-width: 286px;
    margin: auto;
}
.page_right_content.stretch .match_live_view_module{
    padding-left: 25px;
    padding-right: 25px;
    max-width: 451px;
}
.score_header{
    padding: 7px 1px 6px;
    font-size: 0;
    position: relative;
}
.score_header_column{
    display: -webkit-inline-flex;
    display: -ms-inline-flexbox;
    display: inline-flex;
    vertical-align: top;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    width: 50%;
}
.score_header_column.column_left{
    padding-right: 2px;
}
.score_header_column.column_right{
    padding-left: 2px;
    -webkit-justify-content: flex-end;
    -ms-flex-pack: end;
    justify-content: flex-end;
}
.scoreHeader_team{
    transition-property: font-size,line-height;
    transition-duration: .65s;
    transition-timing-function: cubic-bezier(.77, 0, .175, 1);
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    position: relative;
    -webkit-flex: 0 1 auto;
    -ms-flex: 0 1 auto;
    flex: 0 1 auto;
    min-width: 0;
    font-size: 12px;
    color: #e4e4e4;
    line-height: 15px;
    margin-bottom: 2px;
}
.page_right_content.stretch .scoreHeader_team{
    font-size: 14px;
    line-height: 17px;
}
.score_header_column.column_left .scoreHeader_team{
    padding-left: 8px;
    padding-right: 10px;
}
.score_header_column.column_right .scoreHeader_team{
    padding-left: 10px;
    padding-right: 8px;
    text-align: right;
}
.scoreHeader_team_border{
    position: absolute;
    height: 15px;
    margin-top: 1px;
}
.score_header_column.column_left .scoreHeader_team_border{
    border-left: 3px solid #fff;
    left: 0;
}

.score_header_column.column_right .scoreHeader_team_border{
    border-right: 3px solid #fff;
    right: 0;
}
.score_header_column.column_right .scoreHeader_team_border.winner,
.score_header_column.column_left .scoreHeader_team_border.winner{
    border-left: 3px solid #ffdf1b;
}
.scoreHeader_team_text{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.scoreHeader_score_container{
    -webkit-flex: 1 0 auto;
    -ms-flex: 1 0 auto;
    flex: 1 0 auto;
    font-size: 0;
}
.score_header_column.column_left .scoreHeader_score_container{
    text-align: right;
}
.scoreHeader_score{
    transition-property: font-size,line-height;
    transition-duration: .65s;
    transition-timing-function: cubic-bezier(.77, 0, .175, 1);
    display: inline-block;
    vertical-align: middle;
    font-size: 15px;
    line-height: 18px;
    padding: 0 7px;
    min-width: 22px;
    text-align: center;
    color: #ffdf1b;
    background-color: #4d4d4d;
    border-radius: 1px;
}
.page_right_content.stretch .scoreHeader_score{
    font-size: 17px;
    line-height: 24px;
}
.ml1-MatchLiveSoccerModule_PitchContainer {
    width: 100%;
    font-size: 0;
    border: 1px solid #333;
    position: relative;
}
.ml1-SoccerPitch_SVG {
    width: 100%;
    height: auto;
    max-height: 180px;
}
.match_information{
    width: 100%;
    padding-bottom: 17px;
    position: relative;
    z-index: 10;
}
.match_information_container{
    position: relative;
    width: 176px;
    display: block;
    color: #999;
    margin: 0 auto;
    text-align: center;
    cursor: pointer;
}
.match_info_text{
    display: inline;
    font-size: 11px;
    line-height: 13px;
    padding-bottom: 7px;
    position: relative;
}
.match_info_text:hover{
    color: #ddd;
}
.match_info_text:before{
    font-family: bet365UI;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\e823";
    font-size: 11px;
}
.match_info_popup{
    display: none;
    position: absolute;
    z-index: 50;
    width: auto;
