.bs-BtnText_NoMessage {
    color: #000;
    font-size: 13px;
    white-space: nowrap;
    vertical-align: top;
    display: inline-block;
    line-height: 29px;
}
.bs-BtnText_NoMessageSpinner{
    background-image: url(/img/15x15_Yellow.svg);
    background-repeat: no-repeat;
    background-size: 15px 15px;
    -webkit-background-position-y: 3px;
    width: 15px;
    height: 15px;
    -webkit-animation: rotate 1000ms infinite linear;
    animation: rotate 1000ms infinite linear;
    display: inline-block;
    margin: 7px 0 0 7px;
    background-position: 50% 50%;
    vertical-align: middle;
}
@-webkit-keyframes rotate{
    from {
        -webkit-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    to {
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}
@keyframes rotate {
    from {
        -ms-transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
        -o-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    to {
        -ms-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}
